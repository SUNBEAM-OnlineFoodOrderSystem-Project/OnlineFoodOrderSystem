package com.foodapp.controller.restaurant;



@RestController
@RequestMapping("/api/dashboard")
@RequiredArgsConstructor
public class DashboardController {

	
	@Autowired
	private Dashboard
	
	@GetMapping("/summary")
    public DashboardSummaryDTO getSummary() {
        return dashboardService.getSummary();
    }
	
	
	
}
